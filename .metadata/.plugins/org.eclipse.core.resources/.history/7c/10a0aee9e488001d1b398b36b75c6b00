
public class Rental {
	private Movie movie;
	public int daysRented;
	public int getFrequentRentersPoints() {
		return
				movie.getFrequentRenterPoints(daysRented);
	}
	public static void main(String[] args) {
    	Rental r1 = new Rental();
    	Movie m1 = new Movie();
    	
    	m1.priceCode = 1;
    	r1.daysRented = 2;
 
 
    	System.out.println(r1.getFrequentRentersPoints());

	}


}


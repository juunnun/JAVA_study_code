
public class Rental {
	
	public int daysRented;
	Movie m2 = new Movie();
	public int getFrequentRentersPoints() {
		return
				m2.getFrequentRenterPoints(daysRented);
	}


	
	
	public static void main(String[] args) {
		
    	Rental r1 = new Rental();
    	Movie m1 = new Movie();
    	
    	m1.priceCode = 1;
    	
    	r1.daysRented = 8;
 
 
    	System.out.println(r1.getFrequentRentersPoints());

}



}